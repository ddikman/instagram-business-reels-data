{
  "manifest_version": 3,
  "name": "Instagram Reel Stats",
  "version": "1.0",
  "description": "Your extension description.",
  "permissions": ["activeTab", "tabs", "scripting", "clipboardWrite"],
  "host_permissions": ["https://business.facebook.com/*"],
  "action": {
    "default_title": "Scrape reel stats"
  },
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
